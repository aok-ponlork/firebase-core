{
  "ConnectionStrings": {
    "DefaultConnection": "Host=YOUR_HOST;Port=YOUR_PORT;Database=YOUR_DATABASE;Username=YOUR_USERNAME;Password=YOUR_PASSWORD"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Firebase": {
    "CredentialsPath": "Secret/firebase-service.json",
    "ProjectId": "YOUR_FIREBASE_PROJECT_ID",
    "signInWithThirdPartyProvider": "https://identitytoolkit.googleapis.com/v1/accounts:signInWithIdp?key=YOUR_FIREBASE_API_KEY",
    "signInWithPassword": "https://identitytoolkit.googleapis.com/v1/accounts:signInWithPassword?key=YOUR_FIREBASE_API_KEY",
    "RefreshTokenUri": "https://securetoken.googleapis.com/v1/token?key=YOUR_FIREBASE_API_KEY",
    "ValidIssuer": "https://securetoken.google.com/YOUR_FIREBASE_PROJECT_ID",
    "JwksUrl": "https://www.googleapis.com/service_accounts/v1/jwk/securetoken@system.gserviceaccount.com"
  },
  "RootUser": {
    "Email": "YOUR_ROOT_EMAIL",
    "UserName": "YOUR_ROOT_USERNAME",
    "Password": "YOUR_ROOT_PASSWORD"
  },
  "RabbitMQSettings": {
    "HostName": "YOUR_RABBITMQ_HOST",
    "UserName": "YOUR_RABBITMQ_USERNAME",
    "Password": "YOUR_RABBITMQ_PASSWORD",
    "Port": "YOUR_RABBITMQ_PORT:int"
  }
}